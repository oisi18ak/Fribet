<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fribet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForgotPasswordActivity"></activity>
        <activity android:name=".LoggedInActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".ChangePasswordActivityActivity"></activity>
        <activity android:name=".MyBetsActivity"></activity>
        <activity android:name=".BetRequestActivity"></activity>
        <activity android:name=".AllBetsActivity"></activity>
        <activity android:name=".CurrentBetActivity"></activity>
        <activity android:name=".FriendsListActivity"></activity>
        <activity android:name=".AddFriendActivity"></activity>
        <activity android:name=".FriendProfileActivity"></activity>
        <activity android:name=".NewBetActivity"></activity>
    </application>

</manifest>